{
  "presets": ["env"],
  "plugins": [
    ["transform-class-properties"],
    ["transform-object-rest-spread"],
    [
      "transform-runtime",
      {
        "polyfill": false,
        "regenerator": true
      }
    ],
    [
      "transform-react-jsx",
      {
        "pragma": "h"
      }
    ]
  ]
}
